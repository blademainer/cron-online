<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<link rel="shortcut icon" href="favicon.ico">
<title>我行我速电子商务有限公司ERP系统 - 在线Cron表达式生成器佛挡杀佛</title>
<meta name="description" content="通过这个生成器,您可以在线生成任务调度比如Quartz的Cron表达式" />
<meta name="keywords" content="cron creater,generate Cron Expression,Cron Expression online,Quartz Cron Expresssion,cron在线生成工具" />
<link rel="stylesheet" type="text/css" href="style.css">
<link rel="stylesheet" type="text/css" href="easyui.css">
<link rel="stylesheet" type="text/css" href="icon.css">
<script type="text/javascript" src="jquery-1.6.2.min.js"></script>
<script type="text/javascript" src="ZeroClipboard.js"></script>
<script type="text/javascript" src="jquery.easyui.min.js"></script>

<script language="JavaScript">
	var clip = null;
function init() {
	clip = new ZeroClipboard.Client();
	clip.setHandCursor(true);
	clip.addEventListener('load', my_load);
	clip.addEventListener('mouseOver', my_mouse_over);
	clip.addEventListener('complete', my_complete);
	clip.glue('btnCopy');
}
function my_load(client) {
	debugstr("Flash movie loaded and ready.");
}
function my_mouse_over(client) {
alert(4546546);
	// we can cheat a little here -- update the text on mouse over
	clip.setText($('#txtCron').val());
}
function my_complete(client, text) {
	debugstr("Copied text to clipboard: " + text);
}
function debugstr(msg) {
	var copyVal = $('#txtCron').val();
	alert(copyVal);
	
		var p = document.createElement('p');
		p.innerHTML = msg;
		$("#copysuccess").text("已复制到剪贴板");
		$('#d_debug').appendTo(p);
}	
</script>
</head>
<body onLoad="init()">
<div id="cronContainer">
  <div id="tt">
    <H3>我行我速电子商务有限公司ERP系统 - 在线Cron表达式生成器</H3>
  </div>
  <div id="cChoice">
    <div class="easyui-tabs" style="width:700px;height:250px">
      <div title="分钟" style="padding:10px">
        <ul>
          <li>
            <input type="radio" name="rMin" checked="checked" value="*" >
            周期 从
            <input class="easyui-numberspinner" data-options="min:0,max:60" style="width:80px;" value="0" id="minStart">
            分钟开始,每
            <input class="easyui-numberspinner" data-options="min:1,max:60" style="width:80px;" value="0" id="minEnd">
            </input>
            分钟执行一次</li>
          <li>
            <input type="radio" name="rMin" name="zq" value="c">
            指定
            <div id="minPanel"></div>
          </li>
        </ul>
      </div>
      <div title="小时" style="padding:10px">
        <ul>
          <li>
            <input type="radio" name="rHour" id="hourId" checked="checked" value="*">
            每小时
          <li>
            <input type="radio" name="rHour" id="hourId2" value="c">
            指定
            <div id="hourPanel"></div>
          </li>
        </ul>
      </div>
      <div title="日" style="padding:10px">
        <ul>
          <li>
            <input type="radio" name="rDay" id="dayId" checked="checked" value="*">
            每日
          <li>
            <input type="radio" name="rDay" id="dayId2" value="c">
            指定
            <div id="dayPanel"></div>
          </li>
        </ul>
      </div>
      <div title="月" style="padding:10px">
        <ul>
          <li>
            <input type="radio" name="rMon" id="monId" checked="checked" value="?">
            每月
          <li>
            <input type="radio" name="rMon" id="monId2" value="c">
            指定
            <div id="monPanel"></div>
          </li>
        </ul>
      </div>
      <div title="星期" style="padding:10px">
        <ul>
          <li>
            <input type="radio" name="rWeek" id="weekId" checked="checked" value="?">
            每星期
          <li>
            <input type="radio" name="rWeek" id="weekId2" value="c">
            指定
            <div id="weekPanel"></div>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <div id="Cres">
    <fieldset>
    <legend>表达式</legend>
    <div>
      <table>
        <tr>
          <td></td>
          <td>秒</td>
          <td>分钟</td>
          <td>小时</td>
          <td>日</td>
          <td>月</td>
          <td>星期</td>
        </tr>
        <tr>
          <td>表达式字段:</td>
          <td><input id="exSec"/></td>
          <td><input id="exMin"/></td>
          <td><input id="exHour"/></td>
          <td><input id="exDay"/></td>
          <td><input id="exMon"/></td>
          <td><input id="exWeek"/></td>
        </tr>
        <tr>
          <td>Cron 表达式:</td>
          <td colspan="5"><input id="txtCron" style="width:100%"/></td>
          <td><input type="button" value="反解析到UI " id="btnFan"/></td>
        </tr>
        <tr>
          <td colspan="7" style="text-align:center;color:red;font-weight:bold;"><label id="copysuccess"></label></td>
        </tr>
      </table>
    </div>
    </fieldset>
  </div>
  <div id="Cbuttons">
    <input type="button" value="生成 cron表达式"  id="btnGen" icon="icon-search"/>
    <input type="button" value="复制Cron表达式" id="btnCopy"/>
</div></div>
<script src="cron.js"></script>
</div>
</body>
</html>
